execute if entity @s[predicate=classes:checkspell/mage/earth/diamondflesh] at @s run function classes:mage/spells/earth/diamondflesh/cast
execute if entity @s[predicate=classes:checkspell/mage/earth/irongolem] at @s run function classes:mage/spells/earth/irongolem/cast
