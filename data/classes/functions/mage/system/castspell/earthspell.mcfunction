execute if entity @s[predicate=classes:checkspell/mage/earth/ironflesh] at @s run function classes:mage/spells/earth/ironflesh/spell
execute if entity @s[predicate=classes:checkspell/mage/earth/goldflesh] at @s run function classes:mage/spells/earth/goldflesh/spell
execute if entity @s[predicate=classes:checkspell/mage/earth/diamondflesh] at @s run function classes:mage/spells/earth/diamondflesh/spell
execute if entity @s[predicate=classes:checkspell/mage/earth/irongolem] at @s run function classes:mage/spells/earth/irongolem/spell
